html
  include partials/head

  body
    h1 
      div=showData.title

    h2 Developed By
    ul
      for developer in showData.developedBy
        li=developer

    h2 Starring
    ul
      for star in showData.starring
        li=star
      
    h2 Seasons
    ul
      for season in showData.seasons
        li 
          a(class='links', href=`/seasons/${season.number}`)='Season' + ' ' + season.number

      
      

      
